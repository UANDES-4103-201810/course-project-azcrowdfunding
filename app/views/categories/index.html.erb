<p id="notice"><%= notice %></p>

<h1>Categories</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= link_to category.tag, show_by_cat_path(category.tag) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>


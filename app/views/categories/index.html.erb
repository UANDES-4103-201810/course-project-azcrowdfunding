<div class="container">
  <h1 class="animated slideInUp">Categories</h1>
  <br>
  <%= link_to "Outstanding Projects", show_outstanding_path, class: "btn btn-warning btn-lg btn-block animated bounceIn" %>
  <br><br>
  <%= link_to "All", projects_path, class: "btn btn-default btn-lg btn-block animated bounceIn" %>
  <% @categories.each do |category| %>
    <%= link_to category.tag, show_by_cat_path(category.tag), class: "btn btn-default btn-lg btn-block animated bounceIn" %>
  <% end %>
  <br>
</div>



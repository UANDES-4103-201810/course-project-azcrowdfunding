<div class="container">
  <h1>Projects</h1>
  <table class="table">
    <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Goal</th>
      <th>Duration</th>
      <th>Markdown</th>
      <th colspan="3"></th>
    </tr>
    </thead>
    <tbody>
    <% User.all.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>

        <td><%= link_to 'Show', user, :class => "btn btn-link" %></td>
        <td><%= link_to 'Edit', edit_user_registration_path(user), :class => "btn btn-link" %></td>

        <td><%= link_to 'Destroy', user, method: :delete, data: {confirm: 'Are you sure?'}, :class => "btn btn-link" %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>